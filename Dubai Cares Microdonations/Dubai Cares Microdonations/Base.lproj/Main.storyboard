                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" fixedFrame="YES" editable="NO" selectable="NO" translatesAutoresizingMaskIntoConstraints="NO" id="LCQ-o4-2Hc">
                                <rect key="frame" x="20" y="192" width="280" height="279"/>
